msgid "Select or Upload Media"
msgstr "Sélectionner ou charger un média"

#. translators: %s: file name.
#: wp-includes/js/dist/media-utils.js:583
msgid "%s: Sorry, this file type is not supported here."
msgstr "« %s » : désolé, ce type de fichier n’est pas supporté."

#. translators: %s: file name.
#: wp-includes/js/dist/media-utils.js:627
msgid "%s: Sorry, you are not allowed to upload this file type."
msgstr ""
"« %s » : désolé, vous n’êtes pas autorisé·e are charger ce type de fichier."

#. translators: %s: file name.
#: wp-includes/js/dist/media-utils.js:645
msgid "%s: This file is empty."
msgstr "« %s » : ce fichier est vide."

#. translators: %s: file name.
#: wp-includes/js/dist/media-utils.js:656
msgid "%s: This file exceeds the maximum upload size for this site."
msgstr ""
"« %s » : ce fichier dépasse la taille maximale autorisée en chargement sur "
"ce site."

#: wp-includes/js/dist/media-utils.js:679
msgid "Only one file can be used here."
msgstr "Un seul fichier peut être utilisé ici."

#. translators: %s: file name
#: wp-includes/js/dist/media-utils.js:736
msgid "Error while uploading file %s to the media library."
msgstr "Erreur lors du chargement du fichier « %s » dans la médiathèque."

#. translators: %s: file name
#: wp-includes/js/dist/media-utils.js:803
msgid "Error while sideloading file %s to the server."
msgstr "Erreur lors du chargement parallèle du fichier %s sur le serveur."
